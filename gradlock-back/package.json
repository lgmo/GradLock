{
  "name": "gradlock-back",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/server.js",
    "dev": "prisma generate && nodemon",
    "dev:watch": "nodemon",
    "seed": "ts-node prisma/seeds/seed-test.ts",
    "postinstall": "prisma generate",
    "lint": "eslint 'src/**/*.{ts,js}'",
    "format": "prettier --write 'src/**/*.{ts,js,json,md}'",
    "prepare": "husky",
    "test": "node scripts/run-tests.js",
    "test:smoke": "jest --testPathPatterns=smoke --verbose",
    "test:all": "jest --testPathIgnorePatterns=smoke --verbose",
    "test:bdd": "jest --testPathPatterns=bdd --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.11.1",
    "@types/cors": "^2.8.19",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.12",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "eslint": "^9.30.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "jest-cucumber": "^4.5.0",
    "lint-staged": "^16.1.2",
    "nodemon": "^3.1.10",
    "prettier": "3.6.2",
    "prisma": "^6.11.1",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1"
  }
}
